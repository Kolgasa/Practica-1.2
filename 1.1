#include <iostream>
using namespace std;
int main() {
    int x,y;
    x=0;
    y=0;
    char z;
    cin>>x>>y>>z;

    switch ( z ) {

        case '+':
            cout<< x + y;
            break;

        case '-':
            cout<< x - y;
            break;

        case '*':
            cout<< x * y;
            break;

        case '/':
            cout<< x / y;
            break;

        default:
            return 0;
    }
}
